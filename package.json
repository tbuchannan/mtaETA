  },
  "author": "Terence Buchannan",
  "license": "ISC",
  "dependencies": {
    "csvtojson": "^1.1.9",
    "electron": "^9.4.0",
    "es6-promise": "^4.1.1",
    "gtfs-realtime-bindings": "0.0.4",
    "request-promise": "^4.2.2"
  }
}