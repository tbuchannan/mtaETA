{
  "name": "mtaeta",
  "version": "1.0.0",
  "description": "simple ETA app",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build-osx": "electron-packager ./ --platform darwin --out ../dist --overwrite mtaETA [/csv/stations.csv,/csv/stops.csv]",
